<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shinyhunttracker.Controller">
   <left>
      <TreeView fx:id="PokemonList" BorderPane.alignment="CENTER" />
   </left>
   <center>
      <TreeView fx:id="GameList" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <VBox>
         <children>
            <TreeView fx:id="MethodList" prefHeight="327.0" prefWidth="248.0" />
            <HBox prefHeight="25.0" prefWidth="248.0">
               <children>
                  <Label text="Pokemon: ">
                     <HBox.margin>
                        <Insets left="5.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="pokemonLabel" text="Label">
                     <padding>
                        <Insets top="5.0" />
                     </padding>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="248.0">
               <children>
                  <Label text="Game: ">
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="gameLabel" text="Label">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="25.0" prefWidth="248.0">
               <children>
                  <Label text="Method: ">
                     <padding>
                        <Insets left="5.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="methodLabel" text="Label">
                     <padding>
                        <Insets left="5.0" top="5.0" />
                     </padding>
                  </Label>
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </right>
   <bottom>
      <HBox prefHeight="47.0" prefWidth="600.0" BorderPane.alignment="BOTTOM_CENTER">
         <children>
            <Button fx:id="BeginHunt" mnemonicParsing="false" onAction="#buttonClick" text="Button" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <CheckBox mnemonicParsing="false" text="Shiny Charm" HBox.hgrow="ALWAYS" />
            <CheckBox mnemonicParsing="false" text="Lure" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </bottom>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="Save" />
            <Menu mnemonicParsing="false" text="Settings" />
         </menus>
      </MenuBar>
   </top>
</BorderPane>
